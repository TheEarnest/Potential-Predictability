  cd ${funcPath}
  files=`ls --color=no *_func_*`
  for file in ${files}; do
    JobName=ps_func_revisiting_jobs
    if [ "${JobName}" != ${file} ]; then
      sed -i s/"JobName=ps_func_revisiting_jobs
    fi
    tempPrefix=t_ps_func_revisiting_jobs
    if [ "${tempPrefix}" != ${file} ]; then
      sed -i s/"tempPrefix=t_ps_func_revisiting_jobs
    fi
  done


